                      function log() 
                   {    
                   	      event.preventDefault();
                      	const username = document.getElementById('username').value;
                        const password = document.getElementById('password').value;

                                                                                           // Check if the login credentials are correct (you can replace this with your authentication logic)
                       if (username === 'abc' && password === '123') 
                       {
                                                                                          // If credentials are correct, redirect to the welcome page (replace "welcome.html" with your actual welcome page URL)
                             window.location.href = 'test.html';
                       }
                         
  				    
  				        else
  				     {
  					    alert('Invalid username or password. Please try again.');
  				     }
  				   }
  				   const loginForm = document.getElementById('loginForm');
                   loginForm.addEventListener('submit', log);

  				 
